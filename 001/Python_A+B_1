def sumAB(a, b):
    return a + b

def main():
    try:
        num1 = int(input("Enter the first number: "))
        num2 = int(input("Enter the second number: "))

        result = sumAB(num1, num2)
        print(f"The sum of {num1} and {num2} is: {result}")
    except ValueError:
        print("Error: Enter valid numbers.")

if __name__ == "__main__":
    main()
